{
  "name": "sh-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:server": "bun ./src/server/server.ts",
    "dev:client": "vite --host",
    "dev": "run-p dev:* ",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "sql-types": "bun kysely-codegen --dialect postgres",
    "migrate:latest": "tsx ./src/utils/migrate.ts",
    "migrate:down": "tsx ./src/utils/migrate.ts down",
    "new:migration": "tsx ./src/utils/migrationFilename.ts",
    "loadJson": "tsx ./src/utils/parseJson.ts",
    "preview": "vite preview --host"
  },
  "dependencies": {
    "@tanstack/react-query": "latest",
    "@tanstack/react-router": "^1.16.0",
    "@tanstack/router-devtools": "^1.16.0",
    "@tanstack/router-vite-plugin": "^1.16.1",
    "@trpc/client": "next",
    "@trpc/next": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "@types/pg": "^8.11.0",
    "cors": "^2.8.5",
    "kysely": "^0.27.2",
    "kysely-codegen": "^0.11.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.1"
  }
}
